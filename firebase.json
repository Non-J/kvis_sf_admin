{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/favicon.png",
        "destination": "/favicon.png"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}